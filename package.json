{
  "name": "ws-expose-server",
  "main": "server.ts",
  "description": "expose with socket to public internet",
  "version": "0.0.1",
  "private": false,
  "engines": {
    "node": "8.*"
  },
  "scripts": {
    "test": "clear; nodemon;",
    "start": "ts-node server.ts",
    "preinstall": "npm -v;node -v;npm i -g typescript; npm i -g ts-node;"
  },
  "author": "Andrew Brantley",
  "license": "ISC",
  "nodemonConfig": {
    "ext": "ts",
    "ignore": [
      "test/*",
      "docs/*"
    ],
    "delay": "100",
    "exec": "ts-node server"
  },
  "dependencies": {
    "@types/node": "^10.5.1",
    "config": "^1.30.0",
    "express": "^4.16.3",
    "uuid": "^3.3.2",
    "ws": "^5.2.1"
  }
}
